.unity-foldout {
    margin-top: 6px;
    margin-right: 6px;
    margin-bottom: 6px;
    margin-left: 6px;
    padding-top: 6px;
    padding-right: 6px;
    padding-bottom: 10px;
    padding-left: 6px;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    visibility: visible;
    display: flex;
    overflow: visible;
    opacity: 1;
    position: relative;
    bottom: auto;
    -unity-font-style: normal;
    background-color: var(--unity-colors-default-background);
}

#unity-text-input {
    -unity-font-style: normal;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    -unity-text-outline-color: rgba(0, 0, 0, 0);
    -unity-text-align: middle-center;
    -unity-text-outline-width: 0;
    background-color: var(--unity-colors-tab-background-checked);
    color: var(--unity-colors-dropdown-text);
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 13px;
}

.unity-button {
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    margin-top: 10px;
    margin-right: 0;
    margin-bottom: 10px;
    margin-left: 0;
    padding-top: 6px;
    padding-right: 6px;
    padding-bottom: 6px;
    padding-left: 6px;
    font-size: 14px;
    -unity-font-style: bold;
}

.unity-label {
    font-size: 14px;
    -unity-text-align: middle-left;
    -unity-font-style: italic;
}

.unity-foldout__text {
    font-size: 16px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.unity-foldout__input {
    padding-bottom: 12px;
}

.unity-list-view__item {
    font-size: 16px;
    -unity-font-style: normal;
    -unity-text-generator: standard;
}

.unity-tab__header {
    margin-top: 0;
    margin-right: auto;
    margin-bottom: 0;
    margin-left: auto;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    justify-content: flex-start;
    align-items: auto;
    align-self: auto;
    align-content: auto;
}

.unity-tab__header-label {
    -unity-background-image-tint-color: rgb(0, 0, 0);
    -unity-font-style: italic;
    -unity-text-align: middle-left;
    text-overflow: clip;
    white-space: pre;
    flex-shrink: 0;
    overflow: hidden;
    display: flex;
    width: auto;
    max-width: none;
    margin-left: 0;
    margin-right: 0;
    padding-right: 0;
    padding-left: 0;
    padding-top: 0;
    padding-bottom: 0;
    flex-wrap: wrap-reverse;
    flex-direction: column;
}
